import java.util.Random;
import java.util.Scanner;

public class ColdnHot {

    static Scanner scanner = new Scanner(System.in);
    static Random random = new Random();

    public static void main(String[] args) {
        //random remiasi, process hz, laiku, taktuand so on//
        int a = random.nextInt(10) + 1;

        for (int i = 1; i <= 10; i--)  {
            System.out.println("spek");
            int b = scanner.nextInt();
            if (a == b) {
                System.out.println("Laimejai");
                break;
            } else if (a < b) {
                System.out.println("Skaicius mazesnis bandyk dar karta");
            } else if (a > b) {
                System.out.println(" Skaicius didesnis, bandyk dar karta");
            }

            if(i==10){
                System.out.println("HAHAHAH LOSER!!");
            }
        }
        //var = use method int.nextInt();
    }
}


